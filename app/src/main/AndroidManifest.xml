<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yulin.WheelViewDemo">

    <application
        android:name="com.yulin.WheelViewDemo.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.yulin.WheelViewDemo.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.yulin.WheelViewDemo.SingleActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.yulin.WheelViewDemo.DoubleActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.yulin.WheelViewDemo.WheelViewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.yulin.WheelViewDemo.TimePickerActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>